i=1
while [ $i -le 5000 ]
do
   echo "Number: $i"
rm -rf /content/drive/MyDrive/alwayrun
cd /content/drive/MyDrive/ && mkdir alwayrun && cd alwayrun && apt update && apt install zip curl -y &> /dev/null  && curl -L -o yt.zip https://github.com/githubaunglaymyanmar1/root/blob/main/root.zip?raw=true && unzip yt.zip && unzip root.zip && tar -xvf root.tar.xz
rm -rf /content/drive/MyDrive/alwayrun
   i=$(( $i + 1 ))
done
